      "uri": "https://github.com/refaelyehuda/check-duplicate-nuget-packages"
    },
    "repository": {
      "uri": "https://github.com/refaelyehuda/check-duplicate-nuget-packages"
    },
    "issues": {
      "uri": "https://github.com/refaelyehuda/check-duplicate-nuget-packages"
    }
  },

  "branding": {
   "color": "rgb(220, 235, 252)",
	"theme": "light"
},
"files": [
{
	"path": "CheckDuplicatePackages"
